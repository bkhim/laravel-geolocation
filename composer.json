{
  "name": "bkhim/laravel-geolocation",
  "version": "2.1.0",
  "description": "Modern Laravel geolocation package with multiple drivers (IpInfo, MaxMind), Laravel 12+ support, and enhanced features",
  "keywords": ["geolocation", "ip", "location", "laravel", "maxmind", "ipinfo"],
  "homepage": "https://github.com/bkhim/laravel-geolocation",
  "license": "MIT",
  "authors": [
    {
      "name": "Blancos Khim",
      "email": "brian@briankimathi.com",
      "homepage": "https://briankimathi.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.2",
    "ext-openssl": "*",
    "ext-json": "*",
    "illuminate/support": "^5.7|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^6.0|^7.0",
    "geoip2/geoip2": "^3.2"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
    "phpunit/phpunit": "^9.5|^10.5"
  },
  "autoload": {
    "psr-4": {
      "Bkhim\\GeoLocation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bkhim\\GeoLocation\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "ext-maxminddb": "For better performance with MaxMind databases"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "conflict": {
    "adrianorsouza/laravel-geolocation": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bkhim\\GeoLocation\\GeoLocationServiceProvider"
      ],
      "aliases": {
        "GeoLocation": "Bkhim\\GeoLocation\\GeoLocation"
      }
    }
  }
}
