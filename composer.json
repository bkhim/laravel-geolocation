{
  "name": "bkhim/laravel-geolocation",
  "description": "Fork of adrianorsouza/laravel-geolocation with Laravel 12 support",
  "keywords": ["geolocation", "ipinfo", "geoip", "laravel", "laravel-geolocation"],
  "license": "MIT",
  "authors": [
    {
      "name": "Adriano Rosa",
      "email": "info@adrianorosa.com"
    },
    {
      "name": "Your Name",
      "email": "your@email.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.2",
    "ext-openssl": "*",
    "ext-json": "*",
    "illuminate/support": "^5.7|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^6.0|^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
    "phpunit/phpunit": "^9.5|^10.5"
  },
  "autoload": {
    "psr-4": {
      "Adrianorosa\\GeoLocation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Adrianorosa\\GeoLocation\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "geoip2/geoip2": "Required if using MaxMind database driver in the future",
    "ext-maxminddb": "Better performance for MaxMind database reads"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Adrianorosa\\GeoLocation\\GeoLocationServiceProvider"
      ],
      "aliases": {
        "GeoLocation": "Adrianorosa\\GeoLocation\\GeoLocation"
      }
    }
  }
}
