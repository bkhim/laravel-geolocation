{
  "name": "bkhim/laravel-geolocation",
  "version": "4.0.1",
  "description": "Modern Laravel geolocation package with 5 providers (IpInfo, MaxMind, IPStack, IPGeolocation, ipapi.co), 25+ data methods, security detection, and production-ready features for Laravel 10-12",
  "keywords": ["geolocation", "ip", "location", "laravel", "maxmind", "ipinfo", "ipstack", "ipgeolocation", "security", "timezone", "currency", "maps"],
  "homepage": "https://github.com/bkhim/laravel-geolocation",
  "license": "MIT",
  "authors": [
    {
      "name": "Blancos Khim",
      "email": "brian@briankimathi.com",
      "homepage": "https://briankimathi.com/packages/laravel-geolocation"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "ext-openssl": "*",
    "ext-json": "*",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^6.0|^7.0",
    "geoip2/geoip2": "^3.2"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
    "pestphp/pest": "2.x-dev",
    "pestphp/pest-plugin-laravel": "2.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Bkhim\\Geolocation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bkhim\\Geolocation\\Tests\\": "tests/"
    }
  },
  "suggest": {
    "ext-maxminddb": "For better performance with MaxMind databases"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "conflict": {
    "adrianorsouza/laravel-geolocation": "*"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Bkhim\\Geolocation\\GeolocationServiceProvider"
      ],
      "aliases": {
        "GeoLocation": "Bkhim\\Geolocation\\GeoLocation",
        "IpAnonymizer": "Bkhim\\Geolocation\\Facades\\IpAnonymizer",
        "LocationConsentManager": "Bkhim\\Geolocation\\Facades\\LocationConsentManager"
      }
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
